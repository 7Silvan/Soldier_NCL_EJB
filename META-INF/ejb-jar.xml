<?xml version="1.0" encoding="UTF-8"?>
<ejb-jar xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/ejb-jar_3_0.xsd"
         version="3.0">
    <enterprise-beans>
        <entity>
            <display-name>Location</display-name>
            <ejb-name>LocationEJB</ejb-name>
            <home>ncl.military.dao.ejb.LocationHome</home>
            <remote>ncl.military.dao.ejb.Location</remote>
            <ejb-class>ncl.military.dao.ejb.LocationBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>true</reentrant>
            <resource-ref>
                <res-ref-name>location</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
                <mapped-name>jdbc/soldier</mapped-name>
            </resource-ref>
        </entity>
        <entity>
            <display-name>Unit</display-name>
            <ejb-name>UnitEJB</ejb-name>
            <home>ncl.military.dao.ejb.UnitHome</home>
            <remote>ncl.military.dao.ejb.Unit</remote>
            <ejb-class>ncl.military.dao.ejb.UnitBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>true</reentrant>
            <resource-ref>
                <res-ref-name>unit</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
                <mapped-name>jdbc/soldier</mapped-name>
            </resource-ref>
        </entity>
        <entity>
            <display-name>Soldier</display-name>
            <ejb-name>SoldierEJB</ejb-name>
            <home>ncl.military.dao.ejb.SoldierHome</home>
            <remote>ncl.military.dao.ejb.Soldier</remote>
            <ejb-class>ncl.military.dao.ejb.SoldierBean</ejb-class>
            <persistence-type>Bean</persistence-type>
            <prim-key-class>java.lang.String</prim-key-class>
            <reentrant>true</reentrant>
            <resource-ref>
                <res-ref-name>soldier</res-ref-name>
                <res-type>javax.sql.DataSource</res-type>
                <res-auth>Container</res-auth>
                <mapped-name>jdbc/soldier</mapped-name>
            </resource-ref>
        </entity>
    </enterprise-beans>
</ejb-jar>